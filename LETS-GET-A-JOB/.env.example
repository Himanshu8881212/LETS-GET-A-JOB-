# n8n Webhook URLs for AI ATS Evaluator
# Copy this file to .env.local and fill in your actual n8n webhook URLs
# All 4 workflows use WEBHOOK TRIGGERS that respond with JSON

# Job Description Processing Webhook (Workflow: "Job Desc")
# Input: POST { jobUrl: "https://..." }
# Output: { output: "parsed job description text" }
NEXT_PUBLIC_N8N_JD_WEBHOOK_URL=https://your-ngrok-url.ngrok-free.dev/webhook/process-jd

# Resume Processing Webhook (Workflow: "Resume")
# Input: POST with PDF file in multipart/form-data (field name: "data")
# Output: { output: "cleaned resume text" }
NEXT_PUBLIC_N8N_RESUME_WEBHOOK_URL=https://your-ngrok-url.ngrok-free.dev/webhook/process-resume

# Cover Letter Processing Webhook (Workflow: "Cover Letter")
# Input: POST with PDF file in multipart/form-data (field name: "data")
# Output: { output: "cleaned cover letter text" }
NEXT_PUBLIC_N8N_COVER_LETTER_WEBHOOK_URL=https://your-ngrok-url.ngrok-free.dev/webhook/process-cover-letter

# ATS Evaluation Webhook (Workflow: "Eval")
# Input: POST { resume_text: "...", cover_letter_text: "...", job_description: "..." }
# Output: { Full evaluation JSON with scores, fixes, keywords, etc. }
NEXT_PUBLIC_N8N_EVALUATION_WEBHOOK_URL=https://your-ngrok-url.ngrok-free.dev/webhook/evaluate-ats

# Optional: Timeout settings (in milliseconds)
NEXT_PUBLIC_N8N_TIMEOUT=120000

# Optional: Enable debug logging
NEXT_PUBLIC_DEBUG_N8N=false

